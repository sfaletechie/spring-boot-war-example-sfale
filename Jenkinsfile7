pipeline {
  agent any
  tools {
    maven 'Maven'
  }
  stages {
    stage('Build') {
      steps {
        sh 'mvn package'
        sh 'jfrog rt upload --url http://192.168.29.22:8082 --access-token ${ARTIFACTORY_ACCESS_TOKEN} target/hello-world-0.0.1-SNAPSHOT.war java-web-app/'
      }
    }
  }
}
